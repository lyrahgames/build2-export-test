#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBMYTEST_VERSION       $libmytest.version.project_number$ULL
#define LIBMYTEST_VERSION_STR   "$libmytest.version.project$"
#define LIBMYTEST_VERSION_ID    "$libmytest.version.project_id$"
#define LIBMYTEST_VERSION_FULL  "$libmytest.version$"

#define LIBMYTEST_VERSION_MAJOR $libmytest.version.major$
#define LIBMYTEST_VERSION_MINOR $libmytest.version.minor$
#define LIBMYTEST_VERSION_PATCH $libmytest.version.patch$

#define LIBMYTEST_PRE_RELEASE   $libmytest.version.pre_release$

#define LIBMYTEST_SNAPSHOT_SN   $libmytest.version.snapshot_sn$ULL
#define LIBMYTEST_SNAPSHOT_ID   "$libmytest.version.snapshot_id$"
